[Desktop Entry]
Type=Application
Name=Break Timer
Comment=Provides a gentle reminder to take regular breaks
X-Ubuntu-Gettext-Domain=brainbreak
NoDisplay=true
Exec=/usr/bin/brainbreak
AutostartCondition=GSettings org.brainbreak.breaks master-enabled
X-GNOME-Autostart-Phase=Applications
X-GNOME-Autostart-Delay=30
X-GNOME-AutoRestart=true
